    <!-- group for manager -->
    <record id="sale_order_manager" model="res.groups">
         <field name="name">Manager</field>
         <field name="implied_ids" eval="[(4, ref('Magang_base.sale_order_user'))]"/>
         <field name="category_id" ref="Magang_base.sale_order"/>
	</record>
	
	<record model="ir.module.category" id="account_invoice">
            <field name="name">Account Invoice Custom</field>
            <field name="description">The sale Order Application</field>
            <field name="sequence">3</field>
    </record>

